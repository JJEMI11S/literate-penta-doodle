import turtle

frame = turtle.Screen()
frame.setup()
clock = turtle.Turtle()
yeet = turtle.Turtle()
noot = turtle.Turtle()
boop = turtle.Turtle()
danger = turtle.Turtle()
noodle = turtle.Turtle()
nope = turtle.Turtle()
rope = turtle.Turtle()
pupper = turtle.Turtle()
scooter = turtle.Turtle()
snooper = turtle.Turtle()
print(type(snooper))
happy_shoelace = turtle.Turtle()
center = clock.clone()
turtle_army = [clock, yeet, noot, boop, danger, noodle, nope, rope, pupper, scooter, snooper, happy_shoelace]
for i in range(0,len(turtle_army)):
    turtle_army[i].speed(100)
    turtle_army[i] = clock.clone()
    turtle_army[i].left(30*i)
    turtle_army[i].penup()
    turtle_army[i].forward(40)
    turtle_army[i].pendown()
    turtle_army[i].forward(30)
    turtle_army[i].penup()
    turtle_army[i].forward(30)
print("the collective noun for turtles are Bale, Nest, Turn, Dole")
